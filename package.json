{
  "author": "",
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@picocss/pico": "^1.5.2",
    "axios": "^0.27.2",
    "axios-auth-refresh": "^3.3.1",
    "core-js": "^3.8.3",
    "express": "^4.18.1",
    "feather-icons": "^4.29.0",
    "format-package": "^7.0.0",
    "lodash": "^4.17.21",
    "sass-rem": "^3.0.0",
    "serve-static": "^1.15.0",
    "v3-infinite-loading": "^1.0.6",
    "vue": "^3.2.13",
    "vue-cookies": "^1.8.1",
    "vue-feather": "^2.0.0",
    "vue-router": "^4.0.15",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.0.0"
  },
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.6",
    "@volar-plugins/prettier": "^1.1.4",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-vue": "^8.0.3",
    "eslint-plugin-vuejs-accessibility": "^1.2.0",
    "sass": "^1.52.2",
    "sass-loader": "^13.0.0",
    "stylelint": "^14.9.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-scss": "^4.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "plugin:vuejs-accessibility/recommended",
      "plugin:vue/vue3-recommended",
      "@vue/airbnb"
    ],
    "overrides": [
      {
        "files": [
          "src/**/*.js",
          "src/**/*.vue"
        ]
      }
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2022,
      "moduleNameMapper": {
        "eslint/use-at-your-own-risk": "eslint/lib/unsupported-api.js"
      },
      "parser": "@babel/eslint-parser",
      "sourceType": "module"
    },
    "plugins": [
      "vuejs-accessibility",
      "simple-import-sort",
      "prefer-arrow",
      "vue"
    ],
    "root": true,
    "rules": {
      "arrow-body-style": [
        "error",
        "always"
      ],
      "func-names": "off",
      "max-len": "off",
      "no-console": "off",
      "no-debugger": "off",
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 1,
          "maxBOF": 0,
          "maxEOF": 1
        }
      ],
      "object-curly-newline": [
        "error",
        {
          "ExportDeclaration": "always",
          "ImportDeclaration": "never",
          "ObjectExpression": "always",
          "ObjectPattern": "always"
        }
      ],
      "padded-blocks": [
        "error",
        {
          "blocks": "always",
          "classes": "always",
          "switches": "always"
        },
        {
          "allowSingleLineBlocks": true
        }
      ],
      "padding-line-between-statements": [
        "error",
        {
          "blankLine": "always",
          "next": "*",
          "prev": "*"
        },
        {
          "blankLine": "never",
          "next": "const",
          "prev": "const"
        },
        {
          "blankLine": "never",
          "next": "let",
          "prev": "let"
        },
        {
          "blankLine": "always",
          "next": "expression",
          "prev": "expression"
        },
        {
          "blankLine": "any",
          "next": "import",
          "prev": "import"
        },
        {
          "blankLine": "any",
          "next": "cjs-import",
          "prev": "cjs-import"
        }
      ],
      "prefer-arrow-callback": "error",
      "prefer-arrow/prefer-arrow-functions": [
        "error",
        {
          "allowStandaloneDeclarations": false,
          "classPropertiesAllowed": false,
          "disallowPrototype": true,
          "singleReturnOnly": false
        }
      ],
      "quotes": [
        "error",
        "double"
      ],
      "simple-import-sort/exports": "error",
      "simple-import-sort/imports": "error",
      "vue/camelcase": "warn",
      "vue/max-attributes-per-line": [
        "error",
        {
          "multiline": {
            "max": 1
          },
          "singleline": {
            "max": 1
          }
        }
      ],
      "vue/singleline-html-element-content-newline": "off",
      "vuejs-accessibility/heading-has-content": "off",
      "vuejs-accessibility/no-onchange": "off"
    }
  },
  "license": "ISC",
  "main": "index.js",
  "name": "articl-frontent-dev",
  "resolve": {
    "extensions": [
      "*",
      ".js",
      ".vue",
      ".json"
    ]
  },
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "postinstalldd": "npm run build",
    "serve": "vue-cli-service serve",
    "start": "node index.js"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-recommended-vue",
      "stylelint-config-standard",
      "stylelint-config-standard-scss",
      "stylelint-config-recommended-vue/scss",
      "stylelint-config-recess-order",
      "stylelint-config-css-modules",
      "stylelint-config-prettier"
    ],
    "overrides": [
      {
        "files": [
          "*.vue",
          "**/*.vue"
        ],
        "rules": {
          "unit-allowed-list": [
            "em",
            "s"
          ],
          "block-closing-brace-newline-after": "never",
          "block-closing-brace-newline-before": null,
          "block-closing-brace-space-before": "never",
          "block-opening-brace-newline-after": null,
          "block-opening-brace-space-after": "never",
          "color-no-invalid-hex": true
        }
      }
    ],
    "plugins": [
      "stylelint-scss"
    ],
    "rules": {
      "block-closing-brace-empty-line-before": "never",
      "block-closing-brace-newline-after": "never",
      "block-closing-brace-newline-before": null,
      "block-closing-brace-space-before": "never",
      "block-opening-brace-newline-after": null,
      "block-opening-brace-space-after": "never",
      "color-no-invalid-hex": true,
      "unit-allowed-list": [
        "rem",
        "%",
        "s"
      ]
    }
  },
  "type": "module",
  "version": "1.0.0"
}